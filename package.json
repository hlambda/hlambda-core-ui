{
  "name": "frontend-console",
  "version": "0.0.7",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.2.4",
    "@mui/lab": "^5.0.0-alpha.62",
    "@mui/material": "^5.2.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "ansi_up": "^5.1.0",
    "axios": "^0.24.0",
    "base-64": "^1.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.2.0",
    "npm": "^8.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-overlay": "6.0.9",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^8.1.0",
    "recharts": "^2.1.8",
    "styled-components": "^5.3.3",
    "use-http": "^1.0.26",
    "utf8": "^3.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "export REACT_APP_DEVELOPMENT_MODE=true && export REACT_APP_GIT_HEAD_HASH=hotbuild-$(git rev-parse --short=8 HEAD) && react-scripts start",
    "build": "export REACT_APP_GIT_HEAD_HASH=$(git rev-parse --short=8 HEAD) && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-fix": "./node_modules/.bin/eslint --fix --ext .js,.jsx --ignore-pattern public .",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "deploy": "echo \"Deploying to S3\" ; aws s3 sync ./build/ s3://$AWS_S3_BUCKET_NAME ; echo \"Invalidating CDN cache...\" ; aws cloudfront create-invalidation --distribution-id $AWS_CLOUDFRONT_DISTRIBUTION_ID --paths '/*';"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0"
  },
  "homepage": "/console",
  "proxy": "http://localhost:1331"
}